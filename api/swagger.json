        "responses": {
          "200": {
            "description": "An single rating element",
            "schema": {
              "properties": {
                "location": {
                  "properties": {
                    "lat": {
                      "type": "number"
                    },
                    "lng": {
                      "type": "number"
                    }
                  }
                },
                "kpIndex": {
                  "type": "number",
                  "format": "double"
                }
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "schema": {
              "properties": {
                "msg": {
                  "type": "string"
                },
                "code": {
                  "type": "string",
                  "description": "The error code",
                  "default": "INTERNAL",
                  "enum": [
                    "INTERNAL",
                    "KP_SERVICE_ERROR",
                    "WEATHER_SERVICE_ERROR",
                    "SUNSETRISE_SERVICE_ERROR"
                  ]
                }
              }
            }
          }
        }
      }
