      "get": {
        "summary": "get a rating for your location",
        "description": "",
        "x-swagger-router-controller": "Ratings",
        "tags": [
          "Ratings"
        ],
        "operationId": "getRating",
        "parameters": [
          {
            "name": "lng",
            "in": "query",
            "description": "The lat lng position for calculate the rating",
            "required": true,
            "type": "number",
            "format": "double"
          },
          {
            "name": "lat",
            "in": "query",
            "description": "The lat lng position for calculate the rating",
            "required": true,
            "type": "number",
            "format": "double"
          },
          {
            "name": "UTCDateTime",
            "in": "query",
            "description": "The lat lng position for calculate the rating",
            "required": false,
            "type": "string",
            "default": "now",
            "format": "dateTime"
          }
